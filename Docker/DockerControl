# 1. Start alle services
docker-compose up -d

# 2. Se logs fra alle services
docker-compose logs -f

# 3. Se logs fra specifik service
docker-compose logs -f backend

# 4. Stop alle services
docker-compose down

# 5. Genstart en service
docker-compose restart backend

# 6. Rebuild backend efter kode√¶ndringer
docker-compose up -d --build backend
