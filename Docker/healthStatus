# Se status for alle services
docker-compose ps

# Se detaljeret health info
docker inspect --format='{{json .State.Health}}' mosquitto | jq
docker inspect --format='{{json .State.Health}}' questdb | jq
docker inspect --format='{{json .State.Health}}' fastapi-backend | jq
docker inspect --format='{{json .State.Health}}' grafana | jq

# Se logs under opstart
docker-compose up -d && docker-compose logs -f
